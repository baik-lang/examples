tulis("tes rekursif dgn BAIK - Faktorial");

fungsi tesFaktorial(a) {
    isi ret = 1;
    tulis("a start=",a);

    jika (a === 1){
        ret = 1;
    }
    lainnya{
        isi b = tesFaktorial(a - 1);
        ret = a * b;
    }

    tulis ("a end=",a);
    balik ret;
}

isi A = 100;

isi F = tesFaktorial(A);
tulis("F=",F);

tulis("== selesai ==");

